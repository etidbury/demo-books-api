{
  "name": "web",
  "private": true,
  "scripts": {
    "build": "pnpm run clean && pnpm with-env next build",
    "tf-next": "tf-next build",
    "clean": "rimraf .turbo .next out",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ./.env --",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path ../../.gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@acme/books-sdk": "0.1.0",
    "@acme/books-shared": "0.1.0",
    "@acme/common": "0.1.0",
    "@acme/feature-flag-client": "0.1.0",
    "@acme/helper-env": "0.1.0",
    "@acme/helper-sdk-client": "0.1.0",
    "next": "latest",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@acme/eslint-config": "0.1.0",
    "@acme/tailwind-config": "0.1.0",
    "@acme/tsconfig": "0.1.0",
    "@chromatic-com/storybook": "^1.1.6",
    "@storybook/addon-essentials": "8.0.0-beta.2",
    "@storybook/addon-interactions": "8.0.0-beta.2",
    "@storybook/addon-links": "8.0.0-beta.2",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "8.0.0-beta.2",
    "@storybook/nextjs": "8.0.0-beta.2",
    "@storybook/react": "8.0.0-beta.2",
    "@storybook/test": "8.0.0-beta.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.40.0",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "8.4.30",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.1",
    "serve": "^14.2.0",
    "storybook": "8.0.0-beta.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
